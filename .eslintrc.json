{
  "env": {
    "jest": true
  },
  "extends": [
    "@typescript-eslint",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  //   "rules": {
  //     "react/prop-types": "off",
  //     "import/prefer-default-export": "off",
  //     "import/no-cycle": "off",
  //     "no-multi-assign": "off"
  //   },
  "parser": "babel-eslint",
  "settings": {
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    },
    "import/resolver": {
      "typescript": {}
    }
  },
  "plugins": [
    "import",
    "jest-dom"
  ]
}